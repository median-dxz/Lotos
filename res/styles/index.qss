#uploadPage QPushButton {
  margin: 0px;
  height: 20px;
  border-radius: 20px;
  padding: 12px 23px;
  color: #fff;
  background-color: #409eff;
  border-color: #409eff;
  font: 12pt "Microsoft JhengHei";
  font-weight: 800;
  icon-size: 16px;
}

#uploadPage QPushButton:hover {
  background: #66b1ff;
  border-color: #66b1ff;
}

#uploadPage QPushButton:pressed {
  background-color: #409eff;
  border-color: #409eff;
}

#dragBox {
  border: 2px dashed #dcdfe6;
  border-radius: 2px;
}

#uploadProgress {
  height: 10px;
  border-radius: 5%;
  border: none;
  background-color: #ebeef5;
}

#uploadProgress::chunk {
  background-color: #409eff;
  border-radius: 5%;
  border: none;
}
